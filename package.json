    "axios": "^0.21.1",
    "bourbon": "^5.1.0",
    "bourbon-neat": "^3.0.0",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "compression": "1.7.4",
    "connected-react-router": "6.9.1",
    "cross-env": "5.2.1",
    "date-fns": "2.1.0",
